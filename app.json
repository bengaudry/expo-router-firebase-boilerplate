{
  "expo": {
    "scheme": "acme",
    "orientation": "portrait",
    "userInterfaceStyle": "auto",
    "infoPlist": {
      "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets.",
      "UIViewControllerBasedStatusBarAppearance": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "name": "expo-firebase-boilerplate",
    "slug": "expo-firebase-boilerplate",
    "android": {
      "package": "com.ben_gdry.expofirebaseboilerplate"
    },
    "ios": {
      "supportsTablet": true
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
